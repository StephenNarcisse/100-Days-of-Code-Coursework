# Working with range in Python
for number in range(1, 101): #1-100, range does not include max digit.
    if number % 3 == 0 and number % 5 == 0:
        print('FizzBuzz')
    elif number % 3 == 0:
        print('Fizz')
    elif number % 5 == 0:
        print('Buzz')
    else:
        print((f'{number}'))
